Class {
	#name : #GLMEvaluateSelection,
	#superclass : #Announcement,
	#instVars : [
		'action'
	],
	#category : #'Glamour-Morphic-Renderer'
}

{ #category : #accessing }
GLMEvaluateSelection >> action [
	^ action
]

{ #category : #accessing }
GLMEvaluateSelection >> action: anObject [
	action := anObject
]
